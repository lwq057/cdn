{
	"manifest_version": 2,
	"version": "1",
	"name": "虎窝购优惠",
	"short_name": "虎窝购",
	"author": "HVCIS",
	"homepage_url": "https://jd.hooos.com",

	"scope": "https://jd.hooos.com/",
	"start_url": "https://jd.hooos.com/",
	"display": "minimal-ui",
	"lang": "zh-cn",
	"orientation": "any",
	"theme_color": "#ff6b81",
	"background_color": "#fff",

	"description": "京东优惠商品折扣导购.",
	"incognito": "not_allowed",
	"offline_enabled": true,
	"icons": {
		"any": "https://jd.hooos.com/server/static/logo.svg"
	},
	"browser_action":{
		"browser_style": false,
		"default_icon": "https://jd.hooos.com/server/static/logo.svg",
		"default_title": "虎窝拼",
		"default_popup": "https://jd.hooos.com/"
	},
	"chrome_settings_overrides": {
		"homepage": "https://jd.hooos.com",
		"search_provider": {
			"name": "虎窝购",
			"keyword": "商品关键词、商品标题、商品ID、商品链接",
			"search_url": "https://jd.hooos.com/w/?w={searchTerms}",
			"favicon_url": "https://jd.hooos.com/server/static/favicon.ico",
			"suggest_url": "https://suggest.taobao.com/sug?code=utf-8&q={searchTerms}",
			"encoding": "UTF-8",
			"is_default": true
		},
		"startup_pages": ["https://jd.hooos.com"]
	},
	"omnibox": {
		"keyword": "虎窝购"
	}
}
